{
  "name": "advanceedu-ecommerce-api",
  "version": "1.0.1-alpha",
  "description": "Production-ready REST API for AdvanceEdu E-commerce Platform with Stripe integration",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:generate": "prisma generate && ./scripts/disable-typecheck-generated.sh",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "typecheck": "tsc --noEmit",
    "openapi:generate": "tsx src/docs/openapi.ts > openapi.json"
  },
  "keywords": [
    "express",
    "typescript",
    "prisma",
    "stripe",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.17.1",
    "pino": "^10.2.0",
    "pino-http": "^11.0.0",
    "stripe": "^20.1.2",
    "zod": "^4.3.5",
    "zod-openapi": "^5.4.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.8",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitest/coverage-v8": "^4.0.17",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "pino-pretty": "^13.1.3",
    "prisma": "^7.2.0",
    "supertest": "^7.2.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "vitest": "^4.0.17",
    "vitest-mock-extended": "^3.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "overrides": {
    "hono": "^4.11.3"
  }
}